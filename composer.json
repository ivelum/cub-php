{
  "name": "cub/cub",
  "type": "library",
  "description": "A PHP Client for Cub",
  "keywords": ["cub", "api"],
  "license": "MIT",
  "homepage": "https://github.com/ivelum/cub-php",
  "require": {
    "php": ">=5.3",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8 || ^5.0"
  },
  "autoload": {
    "psr-0" : {
        "Cub_" : "lib/"
    }
  },
  "scripts": {
    "tests": [
      "vendor/bin/phpunit --verbose"
    ],
    "tests-travis": [
      "vendor/bin/phpunit --verbose --configuration phpunit.xml"
    ]
  }
}
